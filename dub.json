  "dependencies": {
    "libdparse": "~>0.8.0-alpha.5",
    "ddmp": "~>0.0.1-0.dev.3",
    "unit-threaded": {
      "version": "*",
      "optional": true
    },
    "vibe-d:data": {
      "version": "*",
      "optional": true
    },
    "vibe-d:http": {
      "version": "*",
      "optional": true
    }
  },
  "subPackages": [
    {
      "name": "core",
      "targetType": "library",
      "dependencies": {
        "libdparse": "~>0.8.0-alpha.5",
