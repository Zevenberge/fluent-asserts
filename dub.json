    },
    {
      "name": "trial",
      "sourcePaths": [
        "source"
      ],
      "dependencies": {
        "libdparse": "~>0.8.0-alpha.5",
        "ddmp": "~>0.0.1-0.dev.3",
        "vibe-d:data": "*",
        "vibe-d:http": "*",
        "openssl": "*"
      }
    }
  ]
